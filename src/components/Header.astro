<style>

    /* Sophisticated Header */
    .luxury-header {
        --_header-height:120px;     

        /* position: fixed; */
        position:sticky;
        top: 0;
        left: 0;
        right: 0;
        height: var(--_header-height);
        
        z-index: 1000;
        background: var(--drkbg);
        
        /* background: linear-gradient(to right, rgba(0, 0, 0, 1) 0%, rgba(50, 50, 50, 1) 50%, rgba(0, 0, 0, 1) 100%); */
        /* background: var(--drkbg); */
        color: var(--ltclr);
        /* box-shadow: 0 8px 32px rgba(0, 0, 0, 0.3); */
        box-shadow: 0px 1px 75px rgb(3, 169, 244, .25);
        
    }





    .header-content {
        /* max-width: 1400px; */
        max-width:80%;
        margin: 0 auto;
        height: 100%;
        display: flex;
        justify-content: space-between;
        align-items: center;
        padding: 0 60px;
    }

    .logo {
        
        /* height: 120px; */
        /* height:calc(var(--_header-height) - 10px); */
        height:var(--_header-height);
        filter: drop-shadow(0 0 30px rgba(3, 169, 244, 0.15));
        transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
        color: white;
    }

    

 

    

    .luxury-cta {
        position: relative;
        background: var(--accent);
        border: none;
        padding: 18px 36px;
        color: white;
        font-family: 'Montserrat', sans-serif;
        font-weight: 700;
        font-size: 0.9rem;
        letter-spacing: 1.5px;
        text-transform: uppercase;
        cursor: pointer;
        overflow: hidden;
        transform-style: preserve-3d;
        transition: all 0.4s cubic-bezier(0.23, 1, 0.32, 1);
        clip-path: polygon(12px 0, 100% 0, calc(100% - 12px) 100%, 0 100%);
        box-shadow: 
            0 15px 40px rgba(3, 169, 244, 0.4),
            0 0 60px rgba(3, 169, 244, 0.2);
    }

    .luxury-cta::before {
        content: '';
        position: absolute;
        top: 0;
        left: 0;
        right: 0;
        bottom: 0;
        background: linear-gradient(45deg, transparent, rgba(255, 255, 255, 0.1), transparent);
        transform: translateX(-100%);
        transition: transform 0.8s ease;
    }

    .luxury-cta:hover::before {
        transform: translateX(100%);
    }

    .luxury-cta:hover {
        transform: translateY(-4px) scale(1.05);
        box-shadow: 
            0 25px 60px rgba(3, 169, 244, 0.6),
            0 0 100px rgba(3, 169, 244, 0.4);
    }

    @media (max-width: 768px) {
        .header-content {
            padding: 0 30px;
        }

        .logo {
            height: 60px;
        }

    }
</style>